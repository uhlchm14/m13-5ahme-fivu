    "**/dist": true,
    "**/package.json": true,
    "**/package-lock.json": true,
    "**/gulpfile.js": true,
    "**/tsconfig.json": true,
    "**/tslint.json": true,
    "**/e2e": true,
    "**/karma.conf.js": true,
    "**/README.md": false,
    "**/.angular-cli.json": true,
